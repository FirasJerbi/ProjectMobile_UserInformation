<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"

        android:layout_alignParentStart="true"
        android:layout_marginBottom="351dp"
        android:background="@drawable/insat"
        android:scaleType="centerCrop"
        android:id="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_account_circle_black_18dp"
        android:layout_marginTop="224dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="33dp" />

    <TextView
        android:id="@+id/Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/numIns"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="15dp"
        android:fontFamily="monospace"
        android:hint="user @string/app_namete"
        android:text="Name"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/numIns"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imageView3"
        android:layout_marginEnd="43dp"
        android:layout_marginTop="45dp"
        android:fontFamily="monospace"
        android:hint="user @string/app_namete"
        android:text="numIns"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/CIN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/numIns"
        android:layout_below="@+id/numIns"
        android:layout_marginTop="16dp"
        android:fontFamily="monospace"
        android:hint="user @string/app_namete"
        android:text="CIN"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CIN"
        android:layout_below="@+id/CIN"
        android:layout_marginTop="16dp"
        android:fontFamily="monospace"
        android:hint="user @string/app_namete"
        android:text="Email"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/Email"
        android:layout_below="@+id/Email"
        android:layout_marginTop="16dp"
        android:fontFamily="monospace"
        android:hint="user @string/app_namete"
        android:text="Groupe"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_perm_identity_black_24dp"
        android:layout_alignTop="@+id/numIns"
        android:layout_toStartOf="@+id/numIns"
        android:layout_marginEnd="13dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/ic_credit_card_black_18dp"
        android:layout_alignTop="@+id/CIN"
        android:layout_alignStart="@+id/imageView4"
        android:layout_above="@+id/Email"
        android:layout_alignEnd="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView6"
        android:layout_alignTop="@+id/Email"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/ic_email_12pt_3x"
        android:layout_alignBottom="@+id/Email"
        android:layout_alignEnd="@+id/imageView6" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView7"
        android:layout_alignTop="@+id/Group"
        app:srcCompat="@drawable/ic_group_3x"
        android:layout_alignBottom="@+id/Group"
        android:layout_alignEnd="@+id/imageView7" />

</RelativeLayout>